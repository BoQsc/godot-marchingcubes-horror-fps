[sub_resource type="GDScript" id="GDScript_time_system"]
script/source = "extends Label

var day: int = 1
var hour: int = 13
var minute: int = 0
var timer: float = 0.0

func _process(delta):
	timer += delta
	# Every 4 seconds
	if timer >= 4.0:
		timer = 0.0
		advance_time()

func advance_time():
	# Increment minutes instead of hours
	minute += 1
	
	if minute >= 60:
		minute = 0
		hour += 1
	
	if hour >= 24:
		hour = 0
		day += 1
	
	text = \"Day %d    %02d:%02d\" % [day, hour, minute]
"

